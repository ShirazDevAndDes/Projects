lt();\r\n\r\n    axios.defaults.withCredentials = true;\r\n\r\n    const config = {\r\n      headers: {\r\n        Authorization: `Bearer ${user.token}`,\r\n      },\r\n    };\r\n\r\n    await axios\r\n      .post(\"/addCategory\", formInput, config)\r\n      .then((response) => {\r\n        const res = response.data;\r\n        toast.success(res.success, {\r\n          position: toast.POSITION.TOP_RIGHT,\r\n        });\r\n        // console.log(res.success.length);\r\n\r\n        if (res.success.length > 0) {\r\n          showCategory();\r\n        }\r\n      })\r\n      .catch((err) => {\r\n        const error = err.response.data;\r\n        console.log(error);\r\n      });\r\n  }\r\n\r\n  return (\r\n    <div className=\"px-5\">\r\n      <Helmet>\r\n        <title>Admin: Add Category</title>\r\n        <meta name=\"description\" content=\"Helmet application\" />\r\n      </Helmet>\r\n      <div className=\"card shadow\" style={{ height: \"auto\" }}>\r\n        <div className=\"card-body\">\r\n          <div className=\"h2 p-1\">Add Category</div>\r\n          <form>\r\n            <div className=\"mb-3\">\r\n              <label\r\n                htmlFor=\"add_category\"\r\n                className=\"col-12 ps-2 pb-1 text-black-50\"\r\n              >\r\n                Category Name\r\n              </label>\r\n              <div className=\"col-12\">\r\n                <input\r\n                  type=\"text\"\r\n                  className=\"form-control rounded-pill\"\r\n                  name=\"categoryName\"\r\n                  id=\"categoryName\"\r\n                  onChange={handleInput}\r\n                />\r\n              </div>\r\n            </div>\r\n            <div className=\"mb-5\">\r\n              <div className=\"col-sm-10\">\r\n                <button\r\n                  type=\"submit\"\r\n                  className=\"btn btn-orange\"\r\n                  onClick={handleSubmit}\r\n                >\r\n                  Add Category\r\